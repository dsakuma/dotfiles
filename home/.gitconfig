[push]
	default = simple
	autoSetupRemote = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[pull]
	rebase = true
[core]
	editor = vim
[user]
	name = Daniel Sakuma
	email = dsakuma@gmail.com
[init]
	defaultBranch = main
